{% extends 'layout.html.twig' %}

{% block content %}
<div class="row">
    <div class="col-xs-6">
        <h3>{{ 'form.register'|trans() }}</h3>

        <div>
            {% for flashMessage in app.session.flashbag.get('error') %}
                <div class="alert alert-danger">{{ flashMessage }}</div>
            {% endfor %}
        </div>

        {{ form(form) }}
    </div>
</div>
{% endblock %}